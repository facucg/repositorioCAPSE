#include <stdio.h>

int cubo(int nro);
int cuadrado(int nro);

int main()
{
    int i=0;
    
    printf("x \t x al cuadrado \t x al cubo\n");
    for(i=1; i<=20; i++){
        printf("%d \t %d \t\t %d\n", i, cuadrado(i), cubo(i));
    } 
    
    return 0;
}

int cuadrado(int nro){
    nro = nro * nro;
    
    return nro; 
}

int cubo(int nro){
    nro = cuadrado(nro) * nro;
    
    return nro;
}
